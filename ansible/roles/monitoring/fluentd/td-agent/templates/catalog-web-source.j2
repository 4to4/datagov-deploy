<source>
  type tail
  format apache2
  path /var/log/apache2/ckan.access.log
  pos_file /var/log/td-agent/ckan.access.log
  tag "{{ system }}.apache.access"
</source>
<source>
  type tail
  format apache_error
  path /var/log/apache2/ckan.error.log
  pos_file /var/log/td-agent/ckan.error.log
  tag "{{ system }}.apache.error"
</source>
